<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="pt-BR" id="br.com.gpromo.guiapromo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>GuiaPromo</name>
    <description>As melhores ofertas na palma da sua mao</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">SysSol ADA2 GuiaPromo</author>
    <vs:template-name>BlankJS</vs:template-name>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="10.0" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="www/res/icon/android/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="www/res/icon/android/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="www/res/icon/android/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="www/res/icon/android/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="www/res/icon/android/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="www/res/icon/android/drawable-xxxhdpi-icon.png" />
    </platform>
    <platform name="ios">
        <icon height="180" src="www/res/icons/ios/icon-60-3x.png" width="180" />
        <icon height="60" src="www/res/icons/ios/icon-60.png" width="60" />
        <icon height="120" src="www/res/icons/ios/icon-60-2x.png" width="120" />
        <icon height="76" src="www/res/icons/ios/icon-76.png" width="76" />
        <icon height="152" src="www/res/icons/ios/icon-76-2x.png" width="152" />
        <icon height="40" src="www/res/icons/ios/icon-40.png" width="40" />
        <icon height="80" src="www/res/icons/ios/icon-40-2x.png" width="80" />
        <icon height="57" src="www/res/icons/ios/icon-57.png" width="57" />
        <icon height="114" src="www/res/icons/ios/icon-57-2x.png" width="114" />
        <icon height="72" src="www/res/icons/ios/icon-72.png" width="72" />
        <icon height="144" src="www/res/icons/ios/icon-72-2x.png" width="144" />
        <icon height="29" src="www/res/icons/ios/icon-small.png" width="29" />
        <icon height="58" src="www/res/icons/ios/icon-small-2x.png" width="58" />
        <icon height="50" src="www/res/icons/ios/icon-50.png" width="50" />
        <icon height="100" src="www/res/icons/ios/icon-50-2x.png" width="100" />
    </platform>
    <platform name="windows">
        <icon height="150" src="www/res/icons/windows/Square150x150Logo.scale-100.png" width="150" />
        <icon height="360" src="www/res/icons/windows/Square150x150Logo.scale-240.png" width="360" />
        <icon height="30" src="www/res/icons/windows/Square30x30Logo.scale-100.png" width="30" />
        <icon height="310" src="www/res/icons/windows/Square310x310Logo.scale-100.png" width="310" />
        <icon height="44" src="www/res/icons/windows/Square44x44Logo.scale-100.png" width="44" />
        <icon height="106" src="www/res/icons/windows/Square44x44Logo.scale-240.png" width="106" />
        <icon height="70" src="www/res/icons/windows/Square70x70Logo.scale-100.png" width="70" />
        <icon height="71" src="www/res/icons/windows/Square71x71Logo.scale-100.png" width="71" />
        <icon height="170" src="www/res/icons/windows/Square71x71Logo.scale-240.png" width="170" />
        <icon height="50" src="www/res/icons/windows/StoreLogo.scale-100.png" width="50" />
        <icon height="120" src="www/res/icons/windows/StoreLogo.scale-240.png" width="120" />
        <icon height="150" src="www/res/icons/windows/Wide310x150Logo.scale-100.png" width="310" />
        <icon height="360" src="www/res/icons/windows/Wide310x150Logo.scale-240.png" width="744" />
    </platform>
    <platform name="android">
        <splash qualifier="land-hdpi" src="www/res/screens/android/screen-hdpi-landscape.png" />
        <splash qualifier="land-ldpi" src="www/res/screens/android/screen-ldpi-landscape.png" />
        <splash qualifier="land-mdpi" src="www/res/screens/android/screen-mdpi-landscape.png" />
        <splash qualifier="land-xhdpi" src="www/res/screens/android/screen-xhdpi-landscape.png" />
        <splash qualifier="port-hdpi" src="www/res/screens/android/screen-hdpi-portrait.png" />
        <splash qualifier="port-ldpi" src="www/res/screens/android/screen-ldpi-portrait.png" />
        <splash qualifier="port-mdpi" src="www/res/screens/android/screen-mdpi-portrait.png" />
        <splash qualifier="port-xhdpi" src="www/res/screens/android/screen-xhdpi-portrait.png" />
        <splash src="www/res/screen/android/nuevasplash1.png" width="1242" height="2208" /> 
        <splash src="www/res/screen/android/nuevasplash2.png" width="2208" height="1242" />         
    </platform>
    <platform name="ios">
        <splash height="480" src="www/res/screens/ios/screen-iphone-portrait.png" width="320" />
        <splash height="960" src="www/res/screens/ios/screen-iphone-portrait-2x.png" width="640" />
        <splash height="1024" src="www/res/screens/ios/screen-ipad-portrait.png" width="768" />
        <splash height="2048" src="www/res/screens/ios/screen-ipad-portrait-2x.png" width="1536" />
        <splash height="768" src="www/res/screens/ios/screen-ipad-landscape.png" width="1024" />
        <splash height="1536" src="www/res/screens/ios/screen-ipad-landscape-2x.png" width="2048" />
        <splash height="1136" src="www/res/screens/ios/screen-iphone-568h-2x.png" width="640" />
        <splash height="1334" src="www/res/screens/ios/screen-iphone-portrait-667h.png" width="750" />
        <splash height="2208" src="www/res/screens/ios/screen-iphone-portrait-736h.png" width="1242" />
        <splash height="1242" src="www/res/screens/ios/screen-iphone-landscape-736h.png" width="2208" />
    </platform>
    <platform name="windows">
        <splash height="300" src="www/res/screens/windows/SplashScreen.scale-100.png" width="620" />
        <splash height="1920" src="www/res/screens/windows/SplashScreen.scale-240.png" width="1152" />
        <splash height="1920" src="www/res/screens/windows/SplashScreenPhone.scale-240.png" width="1152" />
    </platform>
    <preference name="Fullscreen" value="True" />
    <platform name="android">
        <preference name="SplashScreen" value="splash.png" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1.2.2" />
    <plugin name="cordova-plugin-device" spec="~2.0.2" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.3" />
    <plugin name="cordova-plugin-network-information" spec="~2.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.2" />
    <plugin name="cordova-plugin-dialogs" spec="~2.0.1" />
    <plugin name="cordova-plugin-splashscreen"  />
    <preference name="ShowTitle" value="True" />
    <preference name="KeepRunning" value="False" />
    <preference name="android-minSdkVersion" value="26" />
    <preference name="android-maxSdkVersion" value="26" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="FadeSplashScreen " value="true" />    
    <feature name="http://api.phonegap.com/1.0/geolocation" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <feature name="http://api.phonegap.com/1.0/notification" />    
    <vs:toolsetVersion>6.3.1</vs:toolsetVersion>
    <engine name="android" spec="5.2.1" />
    <engine name="ios" spec="4.2.0" />
    <engine name="windows" spec="4.4.2" />
</widget>
